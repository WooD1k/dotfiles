alias ..='cd ..'
alias ...='cd ~/'
alias cdp='cd ~/projects'
alias s='cd ~/swift'
alias status='git status'
alias push="git push"
alias pull="git pull"
alias add="git add "
alias revert="git revert"
alias commit='git commit'
alias fetch='git fetch'
alias branch='git branch'
alias clone='git clone'
alias stash='git stash'
alias merge='git merge'
alias checkout="git checkout"
alias tisrv='ts server'
alias tirun='ts run'
alias tsrun='ts run'
alias tsupd='ts @ run --update'
alias tiupd="ts @ run --update"
alias ticlr="ts clear"
alias ticls="ts close"
